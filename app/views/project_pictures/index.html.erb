<p id="notice"><%= notice %></p>

<h1>Listing Project Pictures</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>Image file name</th>
      <th>Image content type</th>
      <th>Image file size</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_pictures.each do |project_picture| %>
      <tr>
        <td><%= project_picture.project %></td>
        <td><%= project_picture.image_file_name %></td>
        <td><%= project_picture.image_content_type %></td>
        <td><%= project_picture.image_file_size %></td>
        <td><%= link_to 'Show', project_picture %></td>
        <td><%= link_to 'Edit', edit_project_picture_path(project_picture) %></td>
        <td><%= link_to 'Destroy', project_picture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project picture', new_project_picture_path %>
